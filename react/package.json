{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prod": "npm run check && npm run build && npm run preview",
    "build": "vite build",
    "preview": "vite preview",
    "format": "npm run concurrently \"npm:check:eslint -- --fix\" \"npm:check:stylelint -- --fix\" \"npm:check:prettier -- --write\"",
    "check": "npm run concurrently npm:check:*",
    "check:type": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "check:prettier": "prettier . --plugin-search-dir . --check",
    "check:eslint": "eslint \"**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}\"",
    "check:stylelint": "stylelint \"**/*.{css,svg,vue}\"",
    "concurrently": "concurrently --prefix-colors auto"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "stylelint": "^15.9.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
