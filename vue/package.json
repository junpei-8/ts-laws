{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "prod": "npm run check && npm run build && npm run preview",
    "build": "vite build",
    "preview": "vite preview",
    "format": "npm run concurrently \"npm:check:eslint -- --fix\" \"npm:check:stylelint -- --fix\" \"npm:check:prettier -- --write\"",
    "check": "npm run concurrently npm:check:*",
    "check:type": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "check:prettier": "prettier . --plugin-search-dir . --check",
    "check:eslint": "eslint \"**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,vue}\"",
    "check:stylelint": "stylelint \"**/*.{css,sass,scss,svg,vue}\"",
    "concurrently": "concurrently --prefix-colors auto"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "stylelint": "^15.9.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
}
